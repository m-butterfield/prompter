enum PaymentPlanName {
  Basic
  Pro
  Premium
}

type PaymentPlanTemplate {
  name: PaymentPlanName!
  price: String!
  queriesPerDay: Int!
}

extend type Query {
  getPaymentPlanTemplates: [PaymentPlanTemplate!]!
}

extend type Mutation {
  getCheckoutSession(paymentPlanName: PaymentPlanName!): String!
}
